<% include header %>

<ol class="breadcrumb">
    <li><a href="/admin">首页</a></li>
    <li><a href="/admin/friendUrlLuck">友情链接管理</a></li>
    <li class="active">修改友情链接内容</li>
  </ol>


<!-- 表格 -->
<div class="row">
	<div class="col-md-12">
		<!-- RECENT PURCHASES -->
        <form class="form-horizontal" method="post" action="/admin/friendUrl_Update/<%= friendUrl._id%>" >
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">友情链接名称</label>
                <div class="col-sm-10">
                    <input type="text"  name="name"  class="add_name form-control" value="<%= friendUrl.name%>">
                </div>
            </div>


            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">友情链接URL</label>
                <div class="col-sm-10">
                    <input type="text"  name="url" required class="form-control"  value="<%= friendUrl.url%>">
                </div>
            </div>
            
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10 ti">
                    <button type="submit"  class="btn btn-default btn_1">确定修改</button>
                </div>
            </div>
        </form>
		<!-- END RECENT PURCHASES -->
	</div>
</div>


<% include footer %>